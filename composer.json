{
    "name": "typehints/laravel-unused",
    "description": "Unused Views,Controllers,.. for Laravel",
    "keywords": ["laravel", "unused"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Benhida",
            "email": "mohamed@typehints.com",
            "homepage": "https://github.com/simoebenhida",
            "role": "Developer"
        },
        {
            "name": "Rachid Laasri",
            "email": "rachid@typehints.com",
            "homepage": "https://github.com/rashidlaasri",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/filesystem":"5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0",
        "illuminate/routing":"5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "nunomaduro/phpinsights": "^1.11",
        "phpstan/phpstan": "^0.12.3"
    },
    "autoload": {
        "psr-4": {
            "TypeHints\\Unused\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TypeHints\\Unused\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TypeHints\\Unused\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
